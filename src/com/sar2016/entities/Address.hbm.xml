<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sar2016.entities">
  <class name="Address" table="Address_Table">
  	 <id name="id" type="long" column="ID_ADDRESS" >
   		<generator class="increment"/>
  	 </id>
	<property name="street" column="STREET"></property>
	<property name="city" column="CITY"></property>
	<property name="zip" column="ZIP"></property>
	<property name="country" column="COUNTRY"></property>
	<property name="placeId" column="PLACE_ID"></property>
  </class>
</hibernate-mapping>
