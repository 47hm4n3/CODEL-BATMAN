<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sar2016.entities">
  <class name="Contact" table="Contact_Table">
  	 <id name="id" type="long" column="ID_CONTACT" >
   		<generator class="increment"/>
  	 </id>
	<property name="firstName" column="FNAME"></property>
	<property name="lastName" column="LNAME"></property>
	<property name="nickName" column="NNAME"></property>
	<property name="email" column="EMAIL" unique="true"></property>
	<!-- A voir l'utilitÃ© ! -->
	
	<many-to-one name="address" column="ID_ADDRESS" unique="true" />
	
	<set name="books" table="TJ_Contact_Groups" inverse="true">
		<key column="ID_CONTACT" />
		<many-to-many class="ContactGroup" column="ID_GROUP" />
	</set>
	
	<set name="profiles" table="Phone_Numbers">
		<key column="ID_CONTACT" />
		<many-to-many class="PhoneNumber" column="ID_PHONE" />
	</set>

  </class>
</hibernate-mapping>
